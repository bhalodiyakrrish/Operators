def utility():
    x=int(input())
    y=int(input())
    p = x+y
    q = x-y
    r = x*y
    s = x/y
    t = x//y
    u = x%y
    v = x**y
    print(p,q,r,"{:.5f}".format(s),t,u,v)
def main():
    t=int(input())
    while(t>0):
        utility()
        t-=1
if __name__ == "__main__": 
    main()

# Input: 
# 1
# 2
# Output: 
# 3 -1 2 0.50000 0 1 1
